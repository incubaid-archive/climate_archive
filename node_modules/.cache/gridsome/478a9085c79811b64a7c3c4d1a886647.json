tive: null,\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    setActive(index) {\r\n      this.active = index;\r\n      this.open = !this.open;\r\n    },\r\n    close(e) {\r\n      if (!this.$el.contains(e.target)) {\r\n        this.open = false;\r\n      }\r\n    },\r\n    resetAll() {\r\n      this.$emit(\"resetAll\", true);\r\n    },\r\n  },\r\n  mounted() {\r\n    document.addEventListener(\"click\", this.close);\r\n  },\r\n  beforeDestroy() {\r\n    document.removeEventListener(\"click\", this.close);\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.top-16 {\r\n  top: 4rem;\r\n}\r\n</style>"]}]}