mbership(filter: {title: {in: [\"foundation\", \"tech\"]}}){\r\n     edges{\r\n      node{\r\n        id\r\n        title\r\n        path\r\n      }\r\n    }\r\n  }\r\n\r\n  }\r\n</page-query>\r\n\r\n<script>\r\nimport PostListItem from \"~/components/custom/Cards/PostListItem.vue\";\r\nimport TagFilterHeader from \"~/components/custom/TagFilterHeader.vue\";\r\n\r\nexport default {\r\n  components: {\r\n    PostListItem,\r\n    TagFilterHeader,\r\n  },\r\n\r\n  computed: {\r\n    memberships() {\r\n      var res = [{ title: \"All\", path: \"/team\" }];\r\n      this.$page.allMembership.edges.forEach((edge) =>\r\n        res.push({ title: edge.node.title, path: edge.node.path })\r\n      );\r\n      return res;\r\n    },\r\n  },\r\n\r\n  metaInfo() {\r\n    return {\r\n      title: this.$page.membership.title,\r\n    };\r\n  },\r\n};\r\n</script>\r\n"]}]}